{
  "name": "do-manager-admin-hooks",
  "version": "1.1.1",
  "description": "Admin hooks for Cloudflare Durable Objects - enables DO Manager integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck"
  },
  "keywords": [
    "cloudflare",
    "durable-objects",
    "workers",
    "admin",
    "management",
    "do-manager"
  ],
  "author": "adamic.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neverinfamous/do-manager-admin-hooks.git"
  },
  "homepage": "https://do.adamic.tech",
  "bugs": {
    "url": "https://github.com/neverinfamous/do-manager-admin-hooks/issues"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "@eslint/js": "^10.0.1",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.55.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  }
}
